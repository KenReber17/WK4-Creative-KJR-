				float2 texCoord16 = IN.ase_texcoord3.xy * float2( 0.7,1 ) + float2( 0,0 );
				float2 panner17 = ( 1.0 * _Time.y * float2( -1,-1 ) + texCoord16);
				float2 texCoord43 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float cos38 = cos( 0.25 * _Time.y );
				float sin38 = sin( 0.25 * _Time.y );
				float2 rotator38 = mul( texCoord43 - float2( 1,0 ) , float2x2( cos38 , -sin38 , sin38 , cos38 )) + float2( 1,0 );
				float cos39 = cos( 0.25 * _Time.y );
				float sin39 = sin( 0.25 * _Time.y );
